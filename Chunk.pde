class Chunk {
  private int x;
  private int y;
  private int hight;

  Chunk(int x, int y, int hight) {
    this.x = x;
    this.y = y;
    this.hight = hight;
  }

  public int getX() {
    return x;
  }

  public int getY() {
    return y;
  }

  public int getHight() {
    return hight;
  }
}
